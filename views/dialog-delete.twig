{% set multiselect = 1 %}
{% set multiselectFilesOnly = false %}
{% set startButtonValue = theUILang.fDiagDeleteBut %}

{% extends "flm::dialog-window.twig" %}
{% block heading %}
    <legend>{{ theUILang.fDiagDeleteSel }}</legend>
{% endblock %}

{% block scripts %}
    <script>
        ((dialogs) => {
            dialogs.onStart((diagId) => flm.actions.doDelete(dialogs.getCheckList(diagId)));
        })(flm.ui.getDialogs());
    </script>
{% endblock %}